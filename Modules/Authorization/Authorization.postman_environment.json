{
  "id": "authorization-env",
  "name": "Authorization Module",
  "values": [
    {
      "key": "base_url",
      "value": "http://localhost:8000",
      "description": "Base URL for the Laravel application",
      "enabled": true
    },
    {
      "key": "api_version",
      "value": "v1",
      "description": "API version",
      "enabled": true
    },
    {
      "key": "authz_prefix",
      "value": "authz",
      "description": "Authorization routes prefix",
      "enabled": true
    },
    {
      "key": "access_token",
      "value": "",
      "description": "JWT access token for authenticated requests",
      "enabled": true
    },
    {
      "key": "app_type",
      "value": "customer",
      "description": "App type for role checking (customer, driver, merchant)",
      "enabled": true
    }
  ],
  "_postman_variable_scope": "environment"
}